{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "nome",
    "endereco",
    "tipoDeCozinha",
    "capacidade",
    "horarioFuncionamento"
  ],
  "properties": {
    "nome": {
      "type": "string"
    },
    "endereco": {
      "type": "object",
      "required": [
        "rua",
        "logradouro",
        "numero",
        "complemento",
        "bairro",
        "cep",
        "uf"
      ],
      "properties": {
        "rua": {
          "type": "string"
        },
        "logradouro": {
          "type": "string"
        },
        "numero": {
          "type": "integer"
        },
        "complemento": {
          "type": "string"
        },
        "bairro": {
          "type": "string"
        },
        "cep": {
          "type": "string"
        },
        "uf": {
          "type": "string"
        }
      }
    },
    "tipoDeCozinha": {
      "type": "string"
    },
    "capacidade": {
      "type": "integer"
    },
    "horarioFuncionamento": {
      "type": "string"
    }
  }
}